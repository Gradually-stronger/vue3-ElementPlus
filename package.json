{
  "name": "website",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prettier": "prettier --write '*.{tsx,ts,less,md,json,vue,css}'",
    "precommit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,md,html,css,vue}": [
      "git add",
      "eslint",
      "prettier --write"
    ],
    "*.css": "stylelint",
    "*.less": "stylelint --syntax=less"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "axios": "^0.27.2",
    "element-plus": "^2.2.2",
    "husky": "^8.0.1",
    "less": "^4.1.2",
    "lint-staged": "^13.0.0",
    "moment": "^2.29.3",
    "particles.vue3": "^1.10.0",
    "prettier": "^2.6.2",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.8.6",
    "unplugin-vue-components": "^0.19.6",
    "vite": "^2.9.9",
    "vue-router": "^4.0.15",
    "vue-tsc": "^0.34.7",
    "vuex": "^4.0.2"
  }
}
